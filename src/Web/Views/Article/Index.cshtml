@model dynamic
@{
    Layout = "_Layout";
}
<div id="articleLogoRowWrapper">
        <div id="articleLogoRow">
            <div class="logo">
                <a href="http://www.3dshooter.com"><svg width="100" height="37">
        <image xlink:href="/dist/app/images/3ds.svg" src="/dist/app/images/3ds.svg" width="100" height="37" />
        </svg></a>
            </div>
            <div class="articleLogoRowItem">
                <a href="mailto:flo@3dshooter.com">Kontakt</a>
            </div>
            <div class="articleLogoRowItem">
                <a href="https://twitter.com/3dshootercom" target="blank">Twitter</a>
            </div>
        </div>
    </div>
<div id="main">
    <h1>Alle Artikel</h1>
    <ul>
    @foreach (var article in Model.Articles)
    {
        <li>
            <a href="/artikel/@article.Id/@article.UrlTitle">@article.Title</a> - @article.Date
        </li>
    }
    </ul>
</div>
